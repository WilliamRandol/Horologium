{
  "name": "Horologium",
  "short_name": "Horologium",
  "version": "0.1",
  "manifest_version": 2,
  "lang": "en",
  "start_url": "/index.html",
  "display": "standalone",
  "background_color": "white",
  "theme_color": "white",
  "app": {
    "background": {
      "scripts": ["/images/background.js"]
    }
  },
  "icons": [
    {
      "src": "/images/icon-192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/images/icon-512.png",
      "sizes": "512x512",
      "type": "image/png"
    }]
}